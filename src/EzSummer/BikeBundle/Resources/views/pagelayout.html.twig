    {% block footer_js %}
    <!-- jQuery -->
    <script src="{{ asset( "bundles/ezsummerbike/js/jquery.min.js" ) }}"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="{{ asset( "bundles/ezsummerbike/js/bootstrap.min.js" ) }}"></script>
    {% endblock %}

</body>
</html>
